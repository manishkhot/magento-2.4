<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<storelocator>
			<general>
				<enable>1</enable>
				<route>storelocator</route>
				<page_title>Store locators</page_title>
				<seo_keywords>keywords</seo_keywords>
				<seo_description>seo description</seo_description>
				<pagination>1</pagination>
				<store_limit>20</store_limit>
				<store_perpage>5</store_perpage>
				<enable_category_filter>1</enable_category_filter>
				<enable_tag_filter>1</enable_tag_filter>
				<api_key>AIzaSyDRVUZdOrZ1HuJFaFkDtmby0E93eJLykIk</api_key>
				<enable_rewrite_url>1</enable_rewrite_url>
				<enable_country_filter>1</enable_country_filter>
				<enable_state_filter>1</enable_state_filter>
				<enable_zipcode_filter>1</enable_zipcode_filter>
				<enable_city_filter>1</enable_city_filter>
				<enable_name_filter>1</enable_name_filter>
				<default_locator_image>lof/storelocator/default.png</default_locator_image>
				<autogeocode>0</autogeocode>
			</general>
			<location>
				<address_preview>57 Trafalgar Square, London WC2N 5DU, UK</address_preview>
				<location_lng>-0.12775829999998223</location_lng>
				<location_lat>51.5073509</location_lat>
				<zoom>12</zoom>
			</location>
			<scheduled_import_settings>
				<enable_cron_tab>1</enable_cron_tab>
				<import_folder>pub/storelocator/</import_folder>
			</scheduled_import_settings>
		</storelocator>
	</default>
</config>